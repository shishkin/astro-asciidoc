{
  "name": "astro-asciidoc",
  "version": "0.1.0",
  "description": "Support AsciiDoc pages in Astro",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": "./dist/types.d.ts",
    "./src/index.js": "./src/index.ts"
  },
  "main": "./dist/index.cjs",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "unbuild",
    "release": "release-it"
  },
  "release-it": {
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "../../CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "chore",
              "hidden": true
            }
          ]
        }
      }
    },
    "git": {
      "requireCleanWorkingDir": false
    },
    "npm": {
      "publish": true,
      "skipChecks": true
    },
    "github": {
      "release": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shishkin/astro-asciidoc.git"
  },
  "homepage": "https://github.com/shishkin/astro-asciidoc",
  "keywords": [
    "astro",
    "astro-integration",
    "withastro",
    "asciidoc",
    "asciidoctor",
    "adoc"
  ],
  "author": "Sergey Shishkin <sergey@shishkin.org>",
  "license": "MIT",
  "dependencies": {
    "@asciidoctor/core": "3.0.4"
  },
  "devDependencies": {
    "astro": "5.16.8",
    "typescript": "5.9.3",
    "unbuild": "3.6.1"
  },
  "peerDependencies": {
    "@astrojs/mdx": "^4.0.0",
    "astro": "^5.0.0",
    "vite": "^4.3.9 || ^5.0.0 || ^6.0.0 || ^7.0.0"
  }
}
